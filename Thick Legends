local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local colors = {
    SchemeColor = Color3.fromRGB(82, 236, 82),
    Background = Color3.fromRGB(75, 95, 107), 
    Header = Color3.fromRGB(45, 58, 65),
    TextColor = Color3.fromRGB(0, 0, 0),
    ElementColor = Color3.fromRGB(143, 253, 143)
}
local Window = Library.CreateLib("0EHUB", colors)
wait(0.1)
local Thick = Window:NewTab("Thick")
local sfarm = Thick:NewSection("Auto Farm")
local util = Thick:NewSection("Utility")
local autopunch = false
sfarm:NewToggle("Auto Punch", "Punches For You", function(state)
    if state then
        autopunch = true
    else
        autopunch = false
    end
end)


local autoeat = false
sfarm:NewToggle("Auto Eat", "Eats For You", function(state)
    if state then
        autoeat = true
    else
        autoeat = false
    end
end)


local autosell = false
sfarm:NewToggle("Auto Sell", "Sells For You", function(state)
    if state then
        autosell = true
    else
        autosell = false
    end
end)

sfarm:NewButton("SPEEDFARM", "visual shouldnt really do anything", function()
    local getGc = getgc
local getInfo = debug.getinfo or getinfo
local getUpvalue = debug.getupvalue or getupvalue or getupval
local getConstants = debug.getconstants or getconstants or getconsts
local isXClosure = is_synapse_function or issentinelclosure or is_protosmasher_closure or is_sirhurt_closure or checkclosure
local isLClosure = islclosure or is_l_closure or (iscclosure and function(f) return not iscclosure(f) end)

assert(getGc and getInfo and getConstants and isXClosure, "Your exploit is not supported")

local function matchConstants(closure, list)
    if not list then
        return true
    end
    
    local constants = getConstants(closure)
    
    for index in pairs(list) do
        if not constants[index] then
            return false
        end
    end
    
    return true
end

local function searchClosure(script, name, upvalueIndex, constants)
    for _i, v in pairs(getGc()) do
        local parentScript = rawget(getfenv(v), "script")

        if type(v) == "function" and 
            isLClosure(v) and 
            not isXClosure(v) and 
            (
                (script == nil and parentScript.Parent == nil) or script == parentScript
            ) 
            and pcall(getUpvalue, v, upvalueIndex)
        then
            if ((name and name ~= "Unnamed function") and getInfo(v).name == name) and matchConstants(v, constants) then
                return v
            elseif (not name or name == "Unnamed function") and matchConstants(v, constants) then
                return v
            end
        end
    end
end

-- stuff
local scriptPath = game:GetService("Players").LocalPlayer.Backpack.Food.FoodToolScript
local closureName = "Unnamed function"
local upvalueIndex = 12
local closureConstants = {
	[1] = "Shops",
	[2] = "Stomach",
	[3] = "EquippedStomach",
	[4] = "Capacity",
	[5] = "Fat",
	[6] = "Name"
}

local closure = searchClosure(scriptPath, closureName, upvalueIndex, closureConstants)
local value = 0.01
local elementIndex = "ReloadTime"

debug.getupvalue(closure, upvalueIndex)[elementIndex] = value
end)

function hatch()
wait(0.2)
local args = {
    [1] = "Chest Pet Stand Red"
}

game:GetService("ReplicatedStorage").Remotes.RequestPurchaseChest:InvokeServer(unpack(args))
end

local autohatch = false
util:NewToggle("Auto Hatch", "Automaticly Hatches Eggs", function(state)
    if state then
        autohatch = true
    else
        autohatch = false
    end
end)

function autobuyf()
if autobuy == true then
local args = {
    [1] = "Fist"
}

game:GetService("ReplicatedStorage").Remotes.RequestBuyAll:FireServer(unpack(args))
local args = {
    [1] = "Food"
}

game:GetService("ReplicatedStorage").Remotes.RequestBuyAll:FireServer(unpack(args))
end
end

local autobuy = false
util:NewToggle("AutoBuy", "Auto Buys Stuff", function(state)
    if state then
        autobuy = true
    else
        autobuy = false
    end
end)

util:NewButton("Unlock all Islands", "TPS you to every island and unlocks it", function()
    for _,child in pairs(game.Workspace:GetChildren()) do
if child.Name == "SellPad" then
local root = game.Players.LocalPlayer.Character.HumanoidRootPart.MassPart
root.CFrame = child.CFrame
wait(0.1)
end end
end)


local universal = Window:NewTab("0EHUB UNIVERSAL")
local LocalPlayer = universal:NewSection("LocalPlayer")
LocalPlayer:NewSlider("WalkSpeed", "Scale Up and down your Walkspeed!", 300, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
LocalPlayer:NewSlider("JumpPower", "Scale Up and down your JumpPower!", 300, 20, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
local noclip = nil
LocalPlayer:NewToggle("Noclip", "Turns On Noclip", function(state)
    if state then
        noclip = true
    else
        noclip = false
    end
end)

game:GetService('RunService').Stepped:connect(function()
if noclip == true then
    game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end end)

while true do
    wait(0.5)
if autohatch == true then
hatch()
end

if autobuy == true then
    autobuyf()
    end

if autoeat == true then
local args = {
    [1] = "Food"
}

game:GetService("ReplicatedStorage").Remotes.UseTool:FireServer(unpack(args))
end
if autopunch == true then
local args = {
    [1] = "Punch"
}

game:GetService("ReplicatedStorage").Remotes.UseTool:FireServer(unpack(args))
end 
if autosell == true then
game:GetService("ReplicatedStorage").Remotes.YieldSell:InvokeServer()
end
end
