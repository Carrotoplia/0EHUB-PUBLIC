local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local colors = {
    SchemeColor = Color3.fromRGB(82, 236, 82),
    Background = Color3.fromRGB(75, 95, 107), -- MAIN BACKGROUND
    Header = Color3.fromRGB(45, 58, 65), -- SIDE Background
    TextColor = Color3.fromRGB(0, 0, 0), -- TEXT COLOR FOR EVERYTHING
    ElementColor = Color3.fromRGB(143, 253, 143)
}
local Window = Library.CreateLib("0EHUB", colors)
wait(0.1)
local universal = Window:NewTab("0EHUB UNIVERSAL")
local LocalPlayer = universal:NewSection("LocalPlayer")
LocalPlayer:NewSlider("WalkSpeed", "Scale Up and down your Walkspeed!", 300, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
LocalPlayer:NewSlider("JumpPower", "Scale Up and down your JumpPower!", 300, 20, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
local noclip = nil
LocalPlayer:NewToggle("Noclip", "Turns On Noclip", function(state)
    if state then
        noclip = true
    else
        noclip = false
    end
end)

game:GetService('RunService').Stepped:connect(function()
if noclip == true then
    game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end end)
